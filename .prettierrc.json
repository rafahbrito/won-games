{
  "trailingComma": "all",
  "semi": false,
  "singleQuote": true,
  "printWidth": 80,
  "overrides": [
    {
      "files": [
        "*.json5"
      ],
      "options": {
        "singleQuote": false,
        "quoteProps": "preserve"
      }
    },
    {
      "files": [
        "*.yml"
